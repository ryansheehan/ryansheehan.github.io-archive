{
  "name": "ryshe-blog",
  "private": true,
  "description": "Source code for personal blog site",
  "version": "0.0.1",
  "author": "Ryan Sheehan <rsheehan@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "rebuild": "run-s clean:cache build",
    "deploy": "run-s clean:cache build deploy:github",
    "deploy:github": "gh-pages -d public -b master -m \"publishing posts [ci skip]\"",
    "develop": "gatsby develop",
    "new": "node post-generator.js",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "run-s clean:cache develop",
    "serve": "gatsby serve",
    "reserve": "run-s rebuild serve",
    "clean:cache": "rimraf .cache",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "gh-pages": "gh-pages"
  },
  "dependencies": {
    "gatsby": "^2.4.2",
    "gatsby-image": "^2.0.41",
    "gatsby-plugin-draft": "0.0.4",
    "gatsby-plugin-feed": "^2.2.0",
    "gatsby-plugin-google-analytics": "^2.0.19",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.36",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-copy-linked-files": "^2.0.12",
    "gatsby-remark-emojis": "^0.3.2",
    "gatsby-remark-images": "^3.0.11",
    "gatsby-remark-prismjs": "^3.2.9",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.33",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.19",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "deepmerge": "^3.2.0",
    "gh-pages": "^2.0.1",
    "js-yaml": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "slugify": "^1.3.4"
  },
  "homepage": "https://ryansheehan.github.io",
  "keywords": [
    "gatsby",
    "markdown",
    "javascript",
    "blog"
  ],
  "license": "MIT"
}
